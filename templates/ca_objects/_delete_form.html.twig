<form method="post" action="{{ path('ca_objects_delete', ca_object.rp ) }}" onsubmit="return confirm('Are you sure you want to delete this ca_object ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ca_object.objectId) }}">
    <button class="btn">Delete</button>
</form>
